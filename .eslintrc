{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base",
      "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "indent": [
          "error",
          4,
          {
              "SwitchCase": 1
          }
      ],
      "semi": [
          2,
          "never"
      ]
  }
}
